{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@layouts/*": [
        "components/layouts/*"
      ],
      "@dashboards/*": [
        "components/layouts/dashboards/*"
      ],
      "*": [
        "node_modules/@types/*"
      ]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
